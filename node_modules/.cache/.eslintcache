n        pg = indReset ? 1 : pagina + 1;\r\n\r\n        api.get('/v1/estabelecimentos', {\r\n            params: {\r\n                codCidade: localStorage.getItem('sessionCodCidade'),\r\n                nome: busca,\r\n                idCategoria: idCategoria,\r\n                idBanner: idBanner,\r\n                pagina: pg\r\n            }\r\n        })\r\n        .then( response => {   \r\n            if (indReset) {\r\n                setResultado(response.data);\r\n                setPagina(1);\r\n            } else {\r\n                setResultado((oldArray) => [...oldArray, ...response.data]);\r\n                setPagina(pagina + 1);\r\n            }\r\n            \r\n            setProcessando(false);            \r\n            setVerMais(response.data.length >= 10);\r\n        })\r\n        .catch(err => {\r\n            console.log(err);\r\n            setProcessando(false);\r\n        });\r\n    }\r\n\r\n    useEffect(() => {\r\n      ListarEstabelecimentos(true);\r\n    }, [location]);\r\n\r\n  return (\r\n    <div className=\"container-fluid mt-page\">\r\n      <NavBar />\r\n\r\n      <div className=\"row m-2\">\r\n        <h3>{descricao}</h3>\r\n        {busca.length > 0 ? <small className=\"mb-4 text-secondary\">Pesquisando por: {busca}</small> : null}\r\n      </div>\r\n\r\n      <div className=\"row m-2\">\r\n        {resultado.map((e) => {\r\n          return (\r\n            <Estabelecimento\r\n              key={e.idEstabelecimento}\r\n              url_img={e.urlLogo}\r\n              nome={e.nome}\r\n              avaliacao={e.avaliacao}\r\n              categoria={e.categoria}\r\n              idEstabelecimento={e.idEstabelecimento}\r\n            />\r\n          );\r\n        })}\r\n      </div>\r\n      \r\n      { processando ? <div className=\"text-center m-5\">\r\n        <span className=\"spinner-grow spinner-grow-sm text-danger\" role=\"status\"></span>\r\n        <span className=\"text-danger ms-2 \">Carregando restaurantes...</span>\r\n      </div> : null}\r\n\r\n      { !processando && verMais ? <div className=\"row m-5\">\r\n        <button onClick={(e) => ListarEstabelecimentos(false)} className=\"btn btn-outline-danger\">\r\n          Ver mais restaurantes\r\n        </button>\r\n      </div> : null}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Busca;","C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\pages\\favoritos\\index.jsx",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\pages\\perfil\\index.jsx",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\pages\\enderecos\\index.jsx",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\components\\endereco\\lista\\index.jsx",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\pages\\pedidos\\index.jsx",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\components\\pedido\\index.jsx",["144"],[],"import Star from \"../../assets/star.png\"\r\nimport \"./style.css\"\r\n\r\nfunction Pedido(props) {\r\n  return <div className=\"col-sm-6 mb-4 p-1 pedido\">\r\n    <a href=\"\">\r\n      <div className=\"row\">\r\n        <div className=\"col-3 \">\r\n          <img className=\"img-pedido\" src={props.url_img} alt=\"\" />\r\n        </div>\r\n        <div className=\"col-9 p-1\">\r\n          <span className=\"d-block\">{props.nome}</span>\r\n          <small className=\"d-block text-danger\">Pedido Nº {props.numero_pedido}</small>\r\n          <small className=\"d-block\">{props.qtd} itens - R$ {props.preco} - {props.data}</small>\r\n          <div>\r\n            <img src={Star} alt=\"\" />\r\n            <img src={Star} alt=\"\" />\r\n            <img src={Star} alt=\"\" />\r\n            <img src={Star} alt=\"\" />\r\n            <img src={Star} alt=\"\" />\r\n\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </a>\r\n\r\n  </div>\r\n}\r\n\r\nexport default Pedido;","C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\pages\\cardapio\\index.jsx",["145"],[],"import NavBar from \"../../components/navbar\";\r\nimport \"./style.css\";\r\nimport Star from \"../../assets/star.png\"\r\nimport Produto from \"../../components/produto/lista/index.jsx\";\r\nimport Footer from \"../../components/footer\";\r\nimport { useParams } from \"react-router-dom\";\r\nimport { useEffect, useState } from \"react\";\r\nimport api from \"../../services/api\";\r\nimport ProdutoModal from \"../../components/produto/modal\";\r\nimport notFav from \"../../assets/favorito.png\";\r\nimport Fav from \"../../assets/favorito2.png\";\r\n\r\nfunction Cardapio(){\r\n\r\n  const {id} = useParams();\r\n  const [nome, setNome] = useState('');\r\n  const [endereco, setEndereco] = useState('');\r\n  const [complemento, setComplemento] = useState('');\r\n  const [bairro, setBairro] = useState('');\r\n  const [cidade, setCidade] = useState('');\r\n  const [uf, setUF] = useState('');\r\n  const [avaliacao, setAvaliacao] = useState(0);\r\n  const [qtdAvaliacao, setQtdAvaliacao] = useState(0);\r\n  const [vlMinPedido, setVlMinPedido] = useState(0);\r\n  const [vlTaxaEntrega, setVlTaxaEntrega] = useState(0);\r\n  const [favorito, setFavorito ] = useState(false);\r\n  const [idFavorito, setIdFavorito ] = useState(0);\r\n\r\n  const [foto, setFoto] = useState('');\r\n  const [categorias, setCategorias] = useState([]);\r\n  const [produtos, setProdutos] = useState([]);\r\n  const [isProdutoOpen, setIsProdutoOpen] = useState(false);\r\n\r\n  useEffect(()=>{\r\n    api.get(`/v1/estabelecimentos/${id}`)\r\n    .then(response => {\r\n      setNome(response.data[0].nome);\r\n      setEndereco(response.data[0].endereco);\r\n      setComplemento(response.data[0].complemento);\r\n      setBairro(response.data[0].bairro);\r\n      setCidade(response.data[0].cidade);\r\n      setUF(response.data[0].uf);\r\n      setAvaliacao(response.data[0].avaliacao);\r\n      setQtdAvaliacao(response.data[0].qtdAvaliacao);\r\n      setFoto(response.data[0].urlFoto);\r\n      setVlMinPedido(response.data[0].vlMinPedido);\r\n      setVlTaxaEntrega(response.data[0].vlTaxaEntrega);\r\n      setFavorito(response.data[0].idFavorito > 0);\r\n      setIdFavorito(response.data[0].idFavorito);\r\n    }).catch(err => {\r\n      console.log(err);\r\n    })\r\n\r\n    api.get(`/v1/cardapios/${id}`)\r\n    .then(response => {\r\n      let categoriasUnica = response.data.map(item => item.categoria);\r\n\r\n      categoriasUnica = categoriasUnica.filter((itemArray, i, arrayCompleto) => {\r\n          return arrayCompleto.indexOf(itemArray) === i;\r\n        })\r\n\r\n        setCategorias(categoriasUnica);\r\n        setProdutos(response.data);\r\n    }).catch(err => {\r\n      console.log(err);\r\n    })\r\n  }, []);\r\n\r\n  function openModalProduto(){\r\n    setIsProdutoOpen(true);\r\n\r\n  };\r\n\r\n  function closeModalProduto(){\r\n    setIsProdutoOpen(false);\r\n  };\r\n\r\n  function Favoritar(){\r\n    api.post('/v1/estabelecimentos/favoritos',{\r\n      id_estabelecimento: id\r\n    }).then(response => {\r\n      setFavorito(true);\r\n      setIdFavorito(response.data.id_favorito)\r\n    }).catch(err => {\r\n      console.log(err);\r\n    })\r\n  }\r\n\r\n  function RemoverFavorito(){\r\n    api.delete(`/v1/estabelecimentos/favoritos/${idFavorito}`)\r\n    .then(response => {\r\n      setFavorito(false);\r\n    }).catch(err => {\r\n      console.log(err);\r\n    })\r\n  }\r\n\r\n  return <div className=\"container-fluid mt-page cardapio\">\r\n    <NavBar />\r\n    <ProdutoModal isOpen={isProdutoOpen}\r\n                  onRequestClose={closeModalProduto}/>\r\n    <div className=\"row col-lg-8 offset-lg-2\">\r\n      <div className=\"col-12\">\r\n        <img className=\"img-fluid rounded img-cardapio\" src={foto} alt=\"\" />\r\n      </div>\r\n\r\n      <div className=\"col-12 mt-4\">\r\n        <div className=\"d-flex justify-content-between\">\r\n          <h2>{nome}</h2>\r\n          <div className=\"favorito\">\r\n            {\r\n              favorito ? <img src={Fav} alt=\"\" onClick={RemoverFavorito}/> : <img src={notFav} alt=\"\" onClick={Favoritar}/>\r\n            } \r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"classificacao\">\r\n          <span>{endereco} {complemento.length > 0 ? ' - ' + complemento : null} - {bairro} - {cidade} - {uf}</span>\r\n        </div>\r\n        <div className=\"mt-2 classificacao\">\r\n          <img src={Star} alt=\"\" />\r\n          <span className=\"ms-1\">{avaliacao.toFixed(1)}</span>\r\n          <span className=\"ms-3\">{qtdAvaliacao} avaliações</span>\r\n        </div>\r\n\r\n        <div className=\"mt-2 classificacao\">\r\n          <span className=\"ms-1\"><b>Taxa de entrega:</b> {new Intl.NumberFormat('pt-BR',{ style: 'currency',\r\n          currency: 'BRL'}).format(vlTaxaEntrega)}</span>\r\n          <span className=\"ms-3\"><b>Pedido mínimo:</b> {new Intl.NumberFormat('pt-BR',{ style: 'currency',\r\n          currency: 'BRL'}).format(vlMinPedido)}</span>\r\n        </div>\r\n      </div>\r\n    \r\n    \r\n      {\r\n        categorias.map(categoria => {\r\n          return <div key={categoria} className=\"row mt-5\">\r\n            <div>\r\n              <h5>{categoria}</h5>\r\n            </div>\r\n            {\r\n              produtos.map(p => {\r\n                return p.categoria === categoria ? \r\n                <Produto  key={p.idProduto} \r\n                          nome={p.nome}\r\n                          descricao={p.descricao}\r\n                          vlProduto={p.vlProduto}\r\n                          vlPromocao={p.vlPromocao}\r\n                          urlFoto={p.urlFoto}\r\n                          onClickProduto={openModalProduto}/> : null\r\n              })\r\n            }\r\n          </div>\r\n        })\r\n      }\r\n    </div>\r\n\r\n    <Footer />\r\n  </div>\r\n}\r\n\r\nexport default Cardapio;","C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\components\\produto\\lista\\index.jsx",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\rotas.js",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\pages\\login\\index.jsx",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\pages\\cadastro\\index.jsx",["146"],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\services\\md5.js",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\services\\api.js",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\components\\produto\\modal\\index.jsx",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\components\\produto\\produto-item-radio\\index.jsx",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\components\\produto\\produto-item-checkbox\\index.jsx",[],[],"C:\\Users\\pedro\\OneDrive\\Área de Trabalho\\PROJETOS\\Trilha - React\\Fontes\\delivery-mais\\src\\components\\endereco\\modal\\index.jsx",["147"],[],{"ruleId":"148","severity":1,"message":"149","line":23,"column":45,"nodeType":"150","endLine":23,"endColumn":80},{"ruleId":"151","severity":1,"message":"152","line":59,"column":8,"nodeType":"153","endLine":59,"endColumn":18,"suggestions":"154"},{"ruleId":"155","severity":1,"message":"156","line":6,"column":5,"nodeType":"150","endLine":6,"endColumn":16},{"ruleId":"151","severity":1,"messag